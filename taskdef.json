{
  "executionRoleArn": "arn:aws:iam::315408206347:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "wp-test-task",
      "image": "<IMAGE1_NAME>",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [ "FARGATE" ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "wp-test-task"
}
